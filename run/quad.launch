<launch>
        <node pkg="refnetclientros" type="refnetclientros" name="refnetclient_phoenix" required="true" output="screen">
                <param name="namespace" type="string" value="phoenix" />

                <!-- IP of the quad. Where does this node pull data from? -->
                <param name="host" type="string" value="localhost" />

                <!-- Which port does this node pull data from? -->
                <param name="port" type="int" value="50000" />
                </node>
</launch>

