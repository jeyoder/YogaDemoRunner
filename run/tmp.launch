<launch>
    <!-- PARAMETERS -->
    <arg name="quad_namespace" value="phoenix"/>
    <arg name="refnet_namespace" value="refnet"/>
    <arg name="use_camera" default="false"/>
    

    <!-- UNIQUE NAMESPACE OF QUAD -->
    <group ns="$(arg quad_namespace)">

        <!-- Snapdragon Nodes -->
        <group>
            <machine 
                name="snapdragon" 
                address="snapdragon" 
                user="root"
                env-loader="/root/remote_env.sh" 
                default="true"/>

            <node 
                machine="snapdragon" 
                name="tmp" 
                pkg="tmp" 
                type="tmp.sh"/>
        </group>
    </group>
</launch>
